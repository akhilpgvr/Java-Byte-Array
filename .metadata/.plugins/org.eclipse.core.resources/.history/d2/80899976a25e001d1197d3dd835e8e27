package exam;
import java.util.ArrayList;
public class FirstProgram{
	void dup(int i) {
		for i
	}
	public static void main(String args[]) {
		ArrayList <Integer> arr = new ArrayList<Integer>();
		arr.add(12);
		arr.add(4);
		arr.add(12);
		arr.add(8);
		arr.add(12);
		System.out.println(arr);
		int n= arr.size();
		for(int i=1;i<n;i++) {
			for(int j=i+1;j<n;j++) {
				if(arr.get(i)==arr.get(j)) {
					arr.remove(i);
				}
			}
		}
		System.out.println(arr);
		
		
	}
}