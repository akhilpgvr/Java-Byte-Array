package exam;
import java.util.ArrayList;
public class FirstProgram{
	public static void main(String args[]) {
		ArrayList <Integer> arr = new ArrayList<Integer>();
		arr.add(12);
		arr.add(4);
		arr.add(12);
		arr.add(8);
		arr.add(12);
		System.out.println(arr);
		int n= arr.size();
		System.out.println(n);
		
		
	}
}